                            <generatePackage>com.ura.casemgt.infrastructure.properties.generated</generatePackage>
                        </configuration>
                        <id>jaxb-generate-app-properties</id>
                    </execution>

                </executions>

                <configuration>
                    <catalog>src/main/resources/jaxb/catalog.xml</catalog>
                    <catalogResolver>org.jvnet.jaxb2.maven2.resolver.tools.ClasspathCatalogResolver</catalogResolver>
                    <forceRegenerate>true</forceRegenerate>
                    <generateDirectory>target/generated-sources/jaxb2</generateDirectory>
