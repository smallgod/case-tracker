                        <goals>
                            <goal>generate</goal>
                        </goals>
                        <configuration>
                            <schemaIncludes>
                                <include>jaxb/app-properties-bindings/v1_0/*.xsd</include>
                            </schemaIncludes>
                            <episodeFile>
                                ${project.build.directory}/generated-sources/xjc/META-INF/jaxb-app-properties.episode
